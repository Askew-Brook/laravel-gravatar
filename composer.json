{
  "name": "askewbrook/laravel-gravatar",
  "description": "Laravel 5 Gravatar helper",
  "keywords": [
    "laravel",
    "laravel5",
    "gravatar"
  ],
  "homepage": "https://github.com/askew-brook/laravel-gravatar",
  "license": "MIT",
  "authors": [
    {
      "name": "Jack Spriggs",
      "email": "jack@askewbrook.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "illuminate/support": "~6.0",
    "thomaswelton/gravatarlib": "0.1.*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "4.8.*",
    "mockery/mockery": "0.9.*"
  },
  "autoload": {
    "psr-4": {
      "Askewbrook\\LaravelGravatar\\": "src/",
      "Askewbrook\\Tests\\LaravelGravatar\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Askewbrook\\LaravelGravatar\\LaravelGravatarServiceProvider"
      ],
      "aliases": {
        "Gravatar": "Askewbrook\\LaravelGravatar\\Facades\\Gravatar"
      }
    }
  },
  "minimum-stability": "stable"
}
